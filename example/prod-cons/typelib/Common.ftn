# Common types for most edges in the flow

Common: module {
  Timespec: structure {
    seconds:  integer(0..1000000000000);
    nanosecs: integer(0..1000000000);
  };

  TimeSlot: integer(range: -1..100; endian: big);
  StreamId: integer(range: 0..10000; bit-size: 16);
  Timestamp: integer(range: 0..255; bit-size: 8);
  Boolean: boolean();
};